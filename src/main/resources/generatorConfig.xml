<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <properties resource="jdbc.properties"/>
  <context id="context1">
    <jdbcConnection connectionURL="${jdbc.url}" driverClass="${jdbc.driverClassName}" password="${jdbc.password}" userId="${jdbc.username}" />
    <javaModelGenerator targetPackage="com.web.demo.model" targetProject="Demo/src/main/java" />
    
    <sqlMapGenerator targetPackage="com.web.demo.mapper" targetProject="Demo/src/main/resources" />
    <javaClientGenerator targetPackage="com.web.demo.mapper" targetProject="Demo/src/main/java" type="XMLMAPPER" />
    <table tableName="cart_item" enableSelectByPrimaryKey="true">
      <generatedKey column="id" sqlStatement="JDBC" identity="true"/>  
    </table>
	
    <table tableName="product" enableSelectByPrimaryKey="true">
      <generatedKey column="id" sqlStatement="JDBC" identity="true"/>  
    </table>   
	<table tableName="customer" enableSelectByPrimaryKey="true">
      <generatedKey column="id" sqlStatement="JDBC" identity="true"/>  
    </table> 
    
   
    <table tableName="account" enableSelectByPrimaryKey="true">
      <generatedKey column="id" sqlStatement="JDBC" identity="true"/>  
    </table>
    <table tableName="account_type" enableSelectByPrimaryKey="true">
      <generatedKey column="id" sqlStatement="JDBC" identity="true"/>  
    </table>
    <table tableName="catalog" enableSelectByPrimaryKey="true">
      <generatedKey column="id" sqlStatement="JDBC" identity="true"/>  
    </table>
    <table tableName="customer_type" enableSelectByPrimaryKey="true">
      <generatedKey column="id" sqlStatement="JDBC" identity="true"/>  
    </table>
    <table tableName="order" enableSelectByPrimaryKey="true">
      <generatedKey column="id" sqlStatement="JDBC" identity="true"/>  
    </table>
    <table tableName="transaction" enableSelectByPrimaryKey="true">
      <generatedKey column="id" sqlStatement="JDBC" identity="true"/>  
    </table>
    
  </context>
</generatorConfiguration>